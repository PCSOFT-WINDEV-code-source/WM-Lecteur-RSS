  procedures :
   -
     name : actualiseListe
     procedure_id : 1970417234612883449
     type_code : 14
     code : |1+
      // Résumé : Actualise la liste des billets
      
      PROCEDURE actualiseListe()
      
      nLargeurContenu est un entier  = LIB_contenu..Largeur 
      nTaillePoliceTitre est un entier = 6 
      nTaillePoliceDescription est un entier = 5
      
      ZoneRépétéeSupprimeTout(ZR_BILLETS)
      
      // Positionne sur le flux
      HLitRecherchePremier(FluxRSS, IDFluxRSS, gnIDFluxRSS)
      
      SI HTrouve(FluxRSS) ALORS
      	
      	//Titre de la fenêtre
      	FEN_BilletListe..Titre = FluxRSS.titre
      	
      	// Initialisation requête pour récupérer tous les billets du flux
      	REQ_BilletListe.pIDFluxRSS = gnIDFluxRSS
      	HExécuteRequête(REQ_BilletListe)
      	
      	pour TOUT REQ_BilletListe 
      		
      		nIndice est un entier = ZoneRépétéeAjouteLigne(ZR_BILLETS, REQ_BilletListe.IDBillet, REQ_BilletListe.Titre,  REQ_BilletListe.Description, "",REQ_BilletListe.Image)	
      		
      		//**Si on n'a pas d'image à afficher, on affiche le texte sur toute la largeur
      		
      		SI REQ_BilletListe.Image = "" ALORS
      			ZR_BILLETS[nIndice].IMG_ILLUS..X = ZR_BILLETS..Largeur - ZR_BILLETS..X 
      		SINON
      			ZR_BILLETS[nIndice].IMG_ILLUS..X = ZR_BILLETS..Largeur - ZR_BILLETS..X - IMG_ILLUS..Largeur - CoordonnéeEditeurVersEcran(10)
      		FIN
      		ZR_BILLETS[nIndice].LIB_CONTENU..Largeur = ZR_BILLETS[nIndice].IMG_ILLUS..X - LIB_CONTENU..X -  CoordonnéeEditeurVersEcran(10)
      		
      		//Libellés qui servent pour calcul des hauteurs de texte
      		LIB_TestHauteurTitre..largeur =  ZR_BILLETS[nIndice].IMG_ILLUS..X - LIB_CONTENU..X -  CoordonnéeEditeurVersEcran(10)
      		LIB_TestHauteurDescription..largeur =  ZR_BILLETS[nIndice].IMG_ILLUS..X - LIB_CONTENU..X -  CoordonnéeEditeurVersEcran(10)
      		
      		//**
      		
      		
      		//Date et heure du billet
      		dhDatePublication est une dateheure = REQ_BilletListe.DatePublication
      		ZR_BILLETS.ATT_DATE_HEURE[nIndice] = DateVersChaîne(dhDatePublication, maskDateDuréeRelative) 
      		
      		//Billet déjà lu ?
      		SI REQ_BilletListe.Lu = vrai ALORS
      			ZR_BILLETS.ATT_CONTENU_COULEUR[nIndice] = grisfoncé
      			ZR_BILLETS.ATT_TITRE_COULEUR[nIndice] = GrisFoncé
      			SI REQ_BilletListe.Image <> "" ALORS 
      				imageBillet est une image = ZR_BILLETS.ATT_IMAGE[nIndice]
      				dModifiesaturation(imageBillet, -50)
      				bufImage est un buffer = imageBillet
      				ZR_BILLETS.ATT_IMAGE[nIndice] = bufImage
      			fin
      		SINON
      			ZR_BILLETS.ATT_CONTENU_COULEUR[nIndice] = RVB(101,128,146)
      		FIN
      		
      		
      		//**Titre et description du billet = contenu du billet
      		//On affiche en priorité le titre entier en plus grosse police
      		//Et s'il reste de la place, on affiche un peu de la description du billet, on tronquera selon la place disponible restante
      		
      		sContenu est une chaine 
      		SI ZR_BILLETS.ATT_Description[nIndice] =  "" ALORS
      			
      			//Si pas de description, c'est facile, on affiche que le titre 
      			sContenu = gStylo(RGB(51,51,51)) + gPoliceTaille(nTaillePoliceTitre) + ZR_BILLETS.ATT_TITRE[nIndice]
      			
      		sinon
      			
      			//S'il y a une description
      			//On détermine le nombre de pixels en hauteur nécessaires pour afficher le titre
      			nTitreHauteur est un entier = TexteHauteur(LIB_TestHauteurTitre,  ZR_BILLETS.ATT_TITRE[nIndice] + rc , tMultiligne)
      			//On en déduit la hauteur restant disponible pour la description du billet
      			nHauteurRestantePourDescr est un entier = LIB_CONTENU..hauteur - nTitreHauteur
      			
      			sDescription est une chaine = formateContenu(ZR_BILLETS.ATT_Description[nIndice]) 
      			
      			n EST UN ENTIER = 1
      			BOUCLE
      				
      				//Tant que la description est trop grande par rapport à la place disponible, on tronque
      				nHauteurDescription est un entier = TexteHauteur(LIB_TestHauteurDescription, sDescription+["... "], tMultiligne)
      				SI nHauteurDescription >= nHauteurRestantePourDescr ALORS
      										
      					//On tronque une première fois en se basant sur un produit en croix
      					nNbreCaractPossible est un entier = (nHauteurRestantePourDescr * taille(sDescription)) / nHauteurDescription
      					sDescription = Tronque(sDescription, Taille(sDescription)-nNbreCaractPossible, nombreDeCaractèresASupprimer) 
      					//					
      					//on tronque intelligemment sur mot complet
      					sDernierMot est une chaine  = extraitchaine(SansEspace(sDescription), rangdernier, " ", depuisfin)
      					SI taille(sDernierMot) >= taille(sDescription) alors sortir
      					sDescription = Tronque(sDescription, Taille(sDernierMot)+1, nombreDeCaractèresASupprimer) 
      					si sDescription = eot ALORS sortir			
      					
      				SINON 
      					SORTIR
      				FIN
      				
      			FIN
      			
      			
      			
      			//S'il reste de la description après la troncature, on montre par "..." que l'on a tronqué
      			SI sDescription <> EOT ALORS
      				sDescription += ["..."]
      				sContenu = gStylo(RGB(51,51,51)) + gPoliceTaille(nTaillePoliceTitre) + ZR_BILLETS.ATT_TITRE[nIndice]+ RC + gStylo(RGB(101,128,146)) + gPoliceTaille(nTaillePoliceDescription) + sDescription
      			SINON
      				sContenu = gStylo(RGB(51,51,51)) + gPoliceTaille(nTaillePoliceTitre) + ZR_BILLETS.ATT_TITRE[nIndice]
      			FIN
      		FIN
      		
      		ZR_BILLETS.ATT_CONTENU[nIndice]  = sContenu
      		
      		//**
      		
      		
      	FIN
      	
      	
      	SI ZR_BILLETS..Occurrence = 0 ALORS
      		Ferme()
      	FIN
      FIN
     type : 458752
