  procedures :
   -
     name : actualise
     procedure_id : 1983440670671648798
     type_code : 14
     code : |1+
      // Résumé : Actualise la liste
      
      PROCEDURE actualise()
      
      ZoneRépétéeSupprimeTout(ZR_FAVORIS)
      
      // Paramètres de la requête 'REQ_BilletListeFavoris'
      REQ_BilletListeFavori.pFavoris = Vrai
      HExécuteRequête(REQ_BilletListeFavori)
      
      POUR TOUT REQ_BilletListeFavori 
      	
      	// Date & Heure
      	sDateHeure est une chaine = ChaîneConstruit("%1 à %2",DateVersChaîne(REQ_BilletListeFavori.DatePublication..PartieDate,"JJ/MM/AAAA"),HeureVersChaîne(REQ_BilletListeFavori.DatePublication..PartieHeure, "HH:MM"))
      	
      	sContenu est une chaine 
      	SI REQ_BilletListeFavori.Description ~= "" ALORS
      		sContenu = "<Aucun contenu>"
      	SINON
      		sContenu= formateContenu(REQ_BilletListeFavori.Description)
      	FIN
      	 
      	nIndice est un entier = ZoneRépétéeAjouteLigne(ZR_FAVORIS, REQ_BilletListeFavori.IDBillet, REQ_BilletListeFavori.Image, sDateHeure, REQ_BilletListeFavori.Titre, sContenu)
      	
      	//Si on n'a pas d'image à afficher, on affiche le texte sur toute la largeur
      	SI REQ_BilletListeFavori.Image = "" ALORS
      		ZR_FAVORIS[nIndice].IMG_ILLUS..X = ZR_FAVORIS..Largeur - ZR_FAVORIS..X 
      	SINON
      		ZR_FAVORIS[nIndice].IMG_ILLUS..X = ZR_FAVORIS..Largeur - ZR_FAVORIS..X - IMG_ILLUS..Largeur - CoordonnéeEditeurVersEcran(10)
      	FIN
      	ZR_FAVORIS[nIndice].LIB_TITRE..Largeur = ZR_FAVORIS[nIndice].IMG_ILLUS..X - LIB_TITRE..X - CoordonnéeEditeurVersEcran(10)
      	ZR_FAVORIS[nIndice].LIB_CONTENU..Largeur = ZR_FAVORIS[nIndice].IMG_ILLUS..X - LIB_CONTENU..X -  CoordonnéeEditeurVersEcran(10)
      	
      	
      FIN
      
