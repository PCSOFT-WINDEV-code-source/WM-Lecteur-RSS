  procedures :
   -
     name : actualise
     procedure_id : 1970420670580966668
     type_code : 14
     code : |1-
      // Résumé : Actualise les informations
      
      PROCEDURE actualise()
      
      
      HLitRecherchePremier(gsSourceRequete, IDbillet, gnid)
      
      
      // Variables
      sHTML         est une chaine
      
      // Affiche les informations
      
      // Positionne sur le billet concerné
      HLitRecherchePremier(Billet,IDBillet,gnID)
      SI HTrouve(Billet) ALORS
      	
      	// Titre du billet
      	LIB_TITRE_BILLET = Billet.Titre
      	
      	tantque TexteHauteur(LIB_TITRE_BILLET,Billet.Titre,tMultiligne) > LIB_TITRE_BILLET..hauteur 
      		LIB_TITRE_BILLET..PoliceTaille -= 1
      	FIN
      	
      	dhHDateBillet est une DateHeure  = {gsSourceRequete + ".DatePublication",indRubrique}
      	LIB_Date_heure = DateVersChaîne(dhHDateBillet..PartieDate, "JJ/MM/AAAA") + " à " + HeureVersChaîne(dhHDateBillet..PartieHeure, "HH:MM")
      	
      	
      	// Passe le billet en lu s'il ne l'était pas
      	SI Billet.Lu = Faux ALORS
      		Billet.Lu = Vrai
      		HModifie(Billet)
      	FIN
      	
      	// Affichage du titre
      	SELON gsSourceFenetre
      		CAS "FEN_BilletListe":
      			MaFenêtre..Titre  = "Tous les billets "  + ChaîneConstruit("(%1/%2)", gnIndiceCourant, gnTotal)
      		CAS "FEN_FavorisListe":
      			MaFenêtre..Titre  = "Tous les favoris "  + ChaîneConstruit("(%1/%2)", gnIndiceCourant, gnTotal)
      	fin
      	
      	//Si on a qu'une image
      	SI Billet.Image <> "" ET Billet.Description = "" ALORS
      		mafenêtre..plan = 2
      		iMG_BILLETseul =  Billet.Image
      	FIN
      	
      	//Si on a que du texte
      	SI Billet.Description <> "" ET Billet.Image = "" ALORS
      		MaFenêtre..Plan = 3
      		HTML_CONTENUSeul = billetHTML(Billet.Description)
      	FIN
      	
      	//Si on a du texte et une image
      	SI Billet.Description <> "" ET Billet.Image <> "" ALORS
      		MaFenêtre..Plan = 1
      		IMG_BILLET =  Billet.Image
      		HTML_CONTENU = billetHTML(Billet.Description)
      	FIN
      	
      	//Si aucun contenu >> on affiche une info 	
      	SI Billet.Image = "" ET Billet.Description = "" ALORS
      		MaFenêtre..Plan = 3
      		sHTML = "Utilisez le bouton 'Lire la suite' pour vous rendre sur la page web."
      		HTML_CONTENUSeul = billetHTML(sHTML)
      	FIN
      	
      	
      	// Active / Désactive bouton selon positionnement
      	SI gnIndiceCourant = 1 ALORS
      		BTN_PRECEDENT..Etat = Grisé
      	SINON
      		BTN_PRECEDENT..Etat = Actif
      	FIN
      	
      	SI gnIndiceCourant = gnTotal ALORS
      		BTN_SUIVANT..Etat = Grisé
      	SINON
      		BTN_SUIVANT..Etat = Actif
      	FIN
      	
      	// Ce billet a un lien
      	SI Billet.Lien <> "" ALORS
      		BTN_VOIR..Etat = Actif
      	SINON
      		BTN_VOIR..Etat = Grisé
      	FIN
      	
      	
      	// Mode favoris ?
      	Si gsSourceFenetre = "FEN_FavorisListe" ALORS
      		
      		img_favori..Visible = Vrai
      		img_nofavori..Visible = faux
      		
      		
      	SINON
      		
      		// Billet favoris ?
      		SI Billet.Favoris = Vrai ALORS
      			IMG_FAVORI..Visible = Vrai
      			IMG_NOFAVORI..Visible = faux
      		SINON
      			IMG_noFAVORI..Visible = vrai 
      			IMG_FAVORI..Visible = faux
      			
      		FIN	
      	FIN
      SINON
      	Erreur("Billet introuvable")
      	Ferme()
      FIN
